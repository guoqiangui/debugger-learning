    "prefix": "debug-react",
    "body": [
      "{",
      "    'name': 'react demo',",
      "    'type': 'chrome',",
      "    'request': 'launch',",
      "    'url': 'http://localhost:${1:3000}',",
      "    'webRoot': '\\${workspaceFolder\\}',",
      "    'runtimeArgs': [",
      "        '--auto-open-devtools-for-tabs',",
      "    ],",
      "},"
    ],
    "description": "调试react项目的配置"
  }
